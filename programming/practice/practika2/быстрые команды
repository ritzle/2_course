-- Вставка данных в таблицу Работники
INSERT INTO Работники VALUES ('тест', 'тест', 'тест')

-- Вставка данных в таблицу Студенты
INSERT INTO Студенты VALUES ('Вероника', 'Быкова', 'Антоновна' '5')

-- Вставка данных в таблицу Предприятия
INSERT INTO Предприятия VALUES ('ОООРогаиКопыта', 'Улица Пушкина дом 1')



-- Выбор данных из таблиц Работники и Студенты с условиями
SELECT Работники.Возраст, Работники.Гендер, Студенты.Балл FROM Работники, Студенты WHERE Работники.Возраст = 25 AND Работники.Гендер = Ж OR Студенты.Балл = 5

-- Выбор 
SELECT Студенты.Имя, FROM Студенты

-- Выбор названий предприятий
SELECT Предприятия.Название FROM Предприятия



-- Удаление работника 
DELETE FROM Работники WHERE Работники.Гендер = null

-- Удаление студента по баллу



DELETE FROM Предприятия WHERE Предприятия.Название = 'ООО Рога и Копыта'




