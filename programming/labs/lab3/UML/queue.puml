@startuml

class Queue {
    - head : Node*
    - size : int

    + Queue()
    + ~Queue()
    + GetSize() : int
    + push(data : T) : void
    + pop() : void
    + print() : void
    + fillFromFile(filename : string) : void
    + writeToFile(filename : string) : void

    class Node {
        - pNext : Node*
        - data : T

        + Node(data : T, pNext : Node*)
    }

    Queue "1" *-- "*" Node : contains
}

@enduml
